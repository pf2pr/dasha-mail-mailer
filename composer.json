{
  "type": "library",
  "name": "pf2pr/dasha-mail-mailer",
  "description": "Symfony DashaMail Mailer Bridge",
  "keywords": ["mailer", "dasha mail"],
  "license": "MIT",
  "authors": [
    {
      "name": "pf2pr",
      "email": "pf2prr@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/mailer": "^6.3"
  },
  "require-dev": {
    "symfony/http-client": "^6.3",
    "symfony/webhook": "^6.3",
    "symfony/remote-event": "^6.3",
    "phpunit/phpunit": "^10.4"
  },
  "autoload": {
    "psr-4": {
      "Pf2Pr\\DashaMailMailer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pf2Pr\\DashaMailMailer\\Tests\\": "Tests/"
    }
  },
  "minimum-stability": "stable"
}
